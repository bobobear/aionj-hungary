				<xs:element name="coupon_trade_info" type="TradeInfo" />
				<xs:element name="extra_currency_trade_info" type="TradeInfo" />
				<xs:element name="html_bg" type="xs:string" />
				<xs:element name="use_script" type="xs:int" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="Appearance">
		<xs:sequence>
			<xs:element name="pc_type" type="xs:string" />
			<xs:element name="face_type" type="xs:int" />
			<xs:element name="face_color" type="xs:string" />
			<xs:element name="hair_type" type="xs:string" />
			<xs:element name="hair_color" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BoundRadius">
		<xs:sequence>
			<xs:element name="front" type="xs:float" />
			<xs:element name="side" type="xs:float" />
			<xs:element name="upper" type="xs:float" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="VisibleEquipments">
		<xs:sequence>
			<xs:element name="head" type="xs:string" />
			<xs:element name="torso" type="xs:string" />
			<xs:element name="leg" type="xs:string" />
			<xs:element name="foot" type="xs:string" />
			<xs:element name="shoulder" type="xs:string" />
			<xs:element name="glove" type="xs:string" />
			<xs:element name="main" type="xs:string" />
			<xs:element name="sub" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TradeInfo">
		<xs:sequence>
			<xs:element name="tab_list" type="TabList" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="sell_price_rate" type="xs:integer"
				minOccurs="0" maxOccurs="1" />
			<xs:element name="buy_price_rate" type="xs:integer"
				minOccurs="0" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TabList">
		<xs:sequence>
			<xs:element name="data" type="Data" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Data">
		<xs:sequence>
			<xs:element name="buy_price_rate" type="xs:integer" />
			<xs:element name="atab" type="xs:string" />
			<xs:element name="ctab" type="xs:string" />
			<xs:element name="etab" type="xs:string" />
			<xs:element name="tab" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>
