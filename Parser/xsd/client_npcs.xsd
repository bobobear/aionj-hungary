				<xs:element name="ammo_fx" type="xs:string" />
				<xs:element name="ammo_hit_fx" type="xs:string" />
				<xs:element name="ammo_speed" type="xs:int" />
				<xs:element name="animation_marker" type="xs:string" />

				<xs:element name="appearance" type="Appearance"
					minOccurs="0" maxOccurs="1" />

				<xs:element name="appearance_custom" type="xs:string" />
				<xs:element name="arrange_midair" type="xs:boolean" />
				<xs:element name="arrange_place" type="xs:string" />
				<xs:element name="arrange_rotation_side" type="xs:boolean" />
				<xs:element name="arrange_rotation_updown" type="xs:boolean" />
				<xs:element name="art_org_move_speed_normal_walk" type="xs:float" />
				<xs:element name="art_org_speed_combat_run" type="xs:float" />
				<xs:element name="artifact_id" type="xs:int" />
				<xs:element name="attack_delay" type="xs:int" />
				<xs:element name="attack_range" type="xs:float" />
				<xs:element name="attack_rate" type="xs:int" />

				<xs:element name="bound_radius" type="BoundRadius"
					minOccurs="0" maxOccurs="1" />

				<xs:element name="cursor_type" type="xs:string" />
				<xs:element name="deadbody_name_id" type="xs:string" />
				<xs:element name="desc" type="xs:string" />
				<xs:element name="dir" type="xs:string" />
				<xs:element name="disk_type" type="xs:string" />
				<xs:element name="dmg_decal_texture" type="xs:string" />
				<xs:element name="dmg_texture" type="xs:int" />
				<xs:element name="erect" type="xs:int" />
				<xs:element name="float_corpse" type="xs:int" />
