				<xs:element name="edit_character_all" type="xs:int" />
				<xs:element name="give_item_proc" type="xs:int" />
				<xs:element name="pvpzone" type="xs:int" />
				<xs:element name="pvpzone_world_name" type="xs:string" />
				<xs:element name="pvpzone_location_alias" type="xs:string" />
