				<xs:element name="mesh" type="xs:string" />
				<xs:element name="move_speed_combat_run" type="xs:float" />
				<xs:element name="move_speed_normal_run" type="xs:float" />
				<xs:element name="move_speed_normal_walk" type="xs:float" />
				<xs:element name="neck_angle" type="xs:float" />
				<xs:element name="npc_type" type="xs:string" />
				<xs:element name="object_type" type="xs:int" />
				<xs:element name="out_time" type="xs:float" />
				<xs:element name="pet_ai_name" type="xs:string" />
				<xs:element name="pushed_range" type="xs:float" />
				<xs:element name="quest_ai_name" type="xs:string" />
				<xs:element name="race_type" type="xs:string" />
				<xs:element name="recovery" type="xs:int" />
				<xs:element name="recovery_opt1" type="xs:int" />
				<xs:element name="recovery_opt2" type="xs:int" />
				<xs:element name="rotation_period" type="xs:float" />
				<xs:element name="sanctuary_animation" type="xs:string" />
