<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
            xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
            jxb:version="2.1">
	<xs:element name="npc_clients">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" ref="npc_client" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="npc_client">
		<xs:complexType>
			<xs:sequence>
